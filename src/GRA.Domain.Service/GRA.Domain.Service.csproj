<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <AssemblyName>GRA.Domain.Service</AssemblyName>
    <PackageId>GRA.Domain.Service</PackageId>
    <NetStandardImplicitPackageVersion>1.6.1</NetStandardImplicitPackageVersion>
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <Version>4.0.0</Version>
    <Copyright>Copyright 2017 Maricopa County Library District</Copyright>
    <PackageLicenseUrl>https://github.com/mcld/greatreadingadventure/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>http://www.greatreadingadventure.com/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mcld/greatreadingadventure/</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <Authors>Maricopa County Library District</Authors>
    <Description>The Great Reading Adventure is a robust, open source software designed to manage library reading programs. The GRA is free to use, modify, and share.</Description>
    <PackageTags>public-library reading-program literacy gra</PackageTags>
    <PackageReleaseNotes>See https://github.com/MCLD/greatreadingadventure/releases/tag/v4.0.0</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\GRA.Domain.Report\GRA.Domain.Report.csproj" />
    <ProjectReference Include="..\GRA\GRA.csproj" />
    <ProjectReference Include="..\GRA.Domain.Model\GRA.Domain.Model.csproj" />
    <ProjectReference Include="..\GRA.Domain.Repository\GRA.Domain.Repository.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="2.16.*" />
    <PackageReference Include="ExcelDataReader" Version="3.*" />
    <PackageReference Include="MailKit" Version="2.*" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="1.*" />
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="1.*" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="1.*" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="1.*" />
    <PackageReference Include="SmartFormat.NET" Version="2.*" />
    <PackageReference Include="System.Security.Claims" Version="4.*" />
    <PackageReference Include="System.ValueTuple" Version="4.*" />
  </ItemGroup>

</Project>
